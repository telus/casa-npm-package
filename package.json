{
	"version": "0.0.0-development",
	"name": "@telus/casa-npm-package",
	"description": "heading component for the casa project",
	"homepage": "https://github.com/telus/casa-npm-package",
	"repository": {
		"type": "git",
		"url": "put git url here"
	},
	"license": "UNLICENSED",
	"main": "dist/index.js",
	"module": "dist/index.es.js",
	"keywords": [
		"telus",
		"heading",
		"casa"
	],
	"engines": {
		"node": ">=14"
	},
	"files": [
		"dist"
	],
	"bugs": {
		"url": "https://github.com/telus/casa-npm-package/issues"
	},
	"scripts": {
		"build": "rollup -c",
		"coverage": "opener coverage/lcov-report/index.html",
		"lint:audit": "npx audit-ci@4.1.0 --config .audit-ci.json",
		"lint:js": "npx eslint --config .eslintrc.js",
		"lint": "npm-run-all -p -c -l lint:*",
		"fix:js": "npm run lint:js -- --fix",
		"fix:deps": "npm run lint:deps -- --update",
		"test": "jest",
		"release:dryrun": "npx semantic-release --dry-run --branch $(git describe --contains --all HEAD)",
		"release": "npx semantic-release",
		"storybook": "start-storybook -p 6006 --no-manager-cache",
		"build-storybook": "build-storybook",
		"prepare": "npm run build"
	},
	"devDependencies": {
		"@babel/core": "^7.17.10",
