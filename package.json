		"@telus/semantic-release-config": "^2.0.7",
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/react": "^12.1.5",
		"babel-loader": "^8.2.5",
		"eslint": "^8.21.0",
		"eslint-config-standard": "^17.0.0",
